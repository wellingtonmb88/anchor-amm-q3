  "version": "0.2.0",
  "configurations": [
  {
    "type": "lldb",
    "request": "launch",
    "name": "Cargo test",
    "cargo": {
      "args": [
        "test",
        "--no-run",
        "--lib"
      ]
    },
    "args": []
  },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'amm'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=amm"
        ],
        "filter": {
          "name": "amm",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
